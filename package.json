{
	"name": "@ctx-core/function",
	"version": "20.2.11",
	"description": "ctx-core function",
	"keywords": [
		"ctx-core",
		"function"
	],
	"homepage": "https://github.com/ctx-core/function#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/function/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/function/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		},
		"./package.json": "./package.json"
	},
	"types": "./src/index.ts",
	"scripts": {
		"build": "npm run compile && npm run copy-mjs",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"copy-mjs": "cd dist && echo \"export * from './index.js'\" > index.mjs",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"type": "module"
}